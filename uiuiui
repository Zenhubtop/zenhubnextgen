					callback = callback or function()

					end

					scrolling_connectY(ScrollingFrame)

					local function join(target)
						if typeof(target) ~= 'table' then
							return tostring(Default or 'None')
						end

						local l = ''

						for i,v in ipairs(target) do
							l = l..' '..tostring(v)..' ,'
						end

						l = l:sub(0,#l - 1)

						return l
					end

